<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LogServlet</servlet-name>
        <servlet-class>TEST.LogServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login_info</servlet-name>
        <servlet-class>login_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup_info</servlet-name>
        <servlet-class>signup_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>controller.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register1</servlet-name>
        <servlet-class>controller.register1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contactusss</servlet-name>
        <servlet-class>controller.contactusss</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contact</servlet-name>
        <servlet-class>controller.contact</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogServlet</servlet-name>
        <url-pattern>/LogServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_info</servlet-name>
        <url-pattern>/login_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup_info</servlet-name>
        <url-pattern>/signup_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register1</servlet-name>
        <url-pattern>/register1</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>contact</servlet-name>
        <url-pattern>/contact</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
